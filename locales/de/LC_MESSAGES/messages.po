msgid "Timezone"
msgstr "Zeitzone"

#: src/server.py:1724
msgid "See"
msgstr "Siehe"

#: src/server.py:1724
msgid "for valid timezone strings"
msgstr "für gültige Zeitzonen Strings"

#: src/server.py:1728
msgid "Date format"
msgstr "Datumsformat"

#: src/server.py:1732
msgid "Valid placeholders: `yyyy`, `mm`, `dd`"
msgstr "Gültige Platzhalter: `yyyy`, `mm`, `dd`"

#: src/server.py:1733
msgid "Example:"
msgstr "Beispiel:"

#: src/server.py:1734 src/server.py:1735
